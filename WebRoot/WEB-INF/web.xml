<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.example.http.HelloServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>RequestDemo1</servlet-name>
		<servlet-class>com.example.http.RequestDemo1</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>RequestDemo2</servlet-name>
		<servlet-class>com.example.http.RequestDemo2</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>RequestDemo3</servlet-name>
		<servlet-class>com.example.http.RequestDemo3</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>RequestDemo4</servlet-name>
		<servlet-class>com.example.http.RequestDemo4</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>RequestDemo5</servlet-name>
		<servlet-class>com.example.http.RequestDemo5</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ResponseDemo1</servlet-name>
		<servlet-class>com.example.http.ResponseDemo1</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ResponseDemo2</servlet-name>
		<servlet-class>com.example.http.ResponseDemo2</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ResponseDemo3</servlet-name>
		<servlet-class>com.example.http.ResponseDemo3</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ResponseDemo4</servlet-name>
		<servlet-class>com.example.http.ResponseDemo4</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>RequestDemo1</servlet-name>
		<url-pattern>/RequestDemo1</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>RequestDemo2</servlet-name>
		<url-pattern>/RequestDemo2</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>RequestDemo3</servlet-name>
		<url-pattern>/RequestDemo3</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>RequestDemo4</servlet-name>
		<url-pattern>/RequestDemo4</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>RequestDemo5</servlet-name>
		<url-pattern>/RequestDemo5</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ResponseDemo1</servlet-name>
		<url-pattern>/ResponseDemo1</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ResponseDemo2</servlet-name>
		<url-pattern>/ResponseDemo2</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ResponseDemo3</servlet-name>
		<url-pattern>/ResponseDemo3</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ResponseDemo4</servlet-name>
		<url-pattern>/ResponseDemo4</url-pattern>
	</servlet-mapping>

</web-app>
